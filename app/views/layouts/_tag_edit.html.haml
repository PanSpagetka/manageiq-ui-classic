- url = url_for_only_path(:action => 'tag_edit_form_field_changed', :id => @sb[:rec_id] || @edit[:object_ids][0])
#tab_div
  #demo-app
  %h3
    - if @tagitems.length > 1
      = _("Tag Assignment (Tags common to all selected items)")
    - else
      = _("Tag Assignment")
  = react 'TaggingWrapper', { :tags => @tags, :urls => @button_urls }
  #demo-app
    :javascript
      miqInitSelectPicker();
      miqSelectPickerEvent("tag_cat", "#{url}", {beforeSend: true, complete: true});
